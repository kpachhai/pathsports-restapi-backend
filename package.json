{
    "name": "pathsports-restapi-backend",
    "version": "0.0.1",
    "description": "Pathsports Rest API",
    "main": "dist/app.js",
    "scripts": {
        "clean": "rimraf dist/*",
        "lint": "prettier --write src/ && tslint -c tslint.json -p tsconfig.json --fix",
        "tsc": "tsc",
        "prestart": "npm run build",
        "build": "npm-run-all clean lint tsc",
        "start": "node dist/app.js",
        "debug": "export DEBUG=* && nodemon --watch src -e ts,ejs --exec npm start",
        "test": "mocha -r ts-node/register 'test/*.test.ts'",
        "test-debug": "export DEBUG=* && npm test"
    },
    "keywords": [
        "REST",
        "API",
        "ExpressJS",
        "NodeJS"
    ],
    "author": "Waqas Ahmed",
    "dependencies": {
        "@types/argon2": "^0.15.0",
        "@types/chai": "^4.2.14",
        "@types/cors": "^2.8.7",
        "@types/debug": "^4.1.5",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.11",
        "@types/helmet": "^4.0.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mocha": "^8.2.0",
        "@types/shortid": "0.0.29",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "argon2": "^0.27.0",
        "chai": "^4.3.3",
        "class-validator": "^0.13.1",
        "cors": "^2.8.5",
        "debug": "^4.2.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^6.9.2",
        "express-winston": "^4.1.0",
        "helmet": "^4.4.1",
        "jsonwebtoken": "^8.5.1",
        "mocha": "^8.3.0",
        "mongoose": "~5.*.*",
        "nodemon": "^2.0.12",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.16",
        "shortid": "^2.2.16",
        "supertest": "^6.1.3",
        "ts-node": "^9.1.1",
        "tslint": "^6.0.0",
        "typescript": "^3.7.5",
        "winston": "^3.3.3"
    },
    "engines": {
        "node": "^12"
    }
}
